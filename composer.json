{
    "name": "tipowerup/ti-theme-orange-tw",
    "type": "tastyigniter-package",
    "description": "Minimal TastyIgniter theme template with Tailwind CSS and Vite",
    "license": "MIT",
    "authors": [
        {
            "name": "TiPowerUp"
        }
    ],
    "keywords": [
        "tastyigniter",
        "theme",
        "tailwind",
        "vite"
    ],
    "require": {
        "tastyigniter/core": "^4.0"
    },
    "require-dev": {
        "laravel/boost": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "TiPowerUp\\OrangeTw\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TiPowerUp\\OrangeTw\\ServiceProvider"
            ]
        },
        "tastyigniter-theme": {
            "code": "tipowerup-orange-tw",
            "name": "Orange TW Theme",
            "locked": false,
            "source-path": "/resources/views",
            "meta-path": "/resources/meta",
            "publish-paths": [
                "/public"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
